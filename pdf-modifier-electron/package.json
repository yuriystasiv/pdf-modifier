{
  "name": "pdf-editor-electron",
  "version": "1.0.0",
  "description": "A macOS PDF editor built with Electron for adding, removing, and reordering PDF pages",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "build": "electron-builder --mac"
  },
  "keywords": [
    "electron",
    "pdf",
    "editor",
    "macos"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^3.11.174"
  },
  "build": {
    "appId": "com.pdfeditor.app",
    "productName": "PDF Editor",
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/icon.icns"
    }
  }
}
